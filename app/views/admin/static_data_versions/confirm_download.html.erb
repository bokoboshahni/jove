<%= render(Jove::TurboDialog::Component.new(title: t('.title'), icon: 'heroicons/outline/cloud-download', dismissable: true)) do |d| %>
  <%= t('.body') %>

  <% d.action do %>
    <%= form_tag admin_static_data_version_download_path(@version) do %>
      <%= render(Jove::Button::Component.new(variant: :text, color: :danger, label: t('.download'), type: :submit, data: { turbo: false })) %>
    <% end %>
  <% end %>
<% end %>
